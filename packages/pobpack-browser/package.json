    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-browser": "./pobpack-browser"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts,.tsx --quiet src/",
    "watch": "pob-watch"
