{
  "name": "pobpack-browser",
  "version": "8.0.4",
  "description": "Build and run browser projects with webpack and webpack-dev-server.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-browser"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "hot": [
        "ts3.1/hot.d.ts"
      ]
    }
  },
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-browser": "./pobpack-browser"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.ts,.tsx --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "cli"
    ],
    "jsx": true
  },
  "peerDependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.7.7",
    "@types/react-hot-loader": "^4.1.1",
    "pobpack-types": "^4.1.1",
    "pobpack-utils": "^8.2.0",
    "react-dev-utils": "^10.0.0",
    "react-error-html": "^0.2.2",
    "regenerator-runtime": "^0.13.3",
    "resolve-from": "^5.0.0",
    "source-map-support": "^0.5.16",
    "webpack-dev-server": "3.10.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "@pob/eslint-config": "37.1.0",
    "@pob/eslint-config-babel-node": "37.0.2",
    "@pob/eslint-config-node": "37.0.2",
    "@pob/eslint-config-typescript": "37.1.0",
    "@pob/eslint-config-typescript-react": "37.1.0",
    "@types/react-dev-utils": "9.0.2",
    "@types/webpack-dev-server": "3.9.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "babel-preset-latest-node": "4.1.0",
    "eslint": "6.8.0",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-unicorn": "18.0.1",
    "pob-babel": "24.12.0",
    "prettier": "2.0.5",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "webpack": "4.41.3"
  }
}
