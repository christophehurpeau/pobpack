{
  "name": "pobpack-browser",
  "version": "7.5.1",
  "description": "Build and run browser projects with webpack and webpack-dev-server.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/pobpack.git",
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "BrowserAppContainer.js": ["ts3.1/*"]
    }
  },
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-browser": "./pobpack-browser"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts,.tsx src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@types/react-hot-loader": "^4.1.0",
    "pobpack-types": "^3.2.2",
    "pobpack-utils": "^7.3.2",
    "react-dev-utils": "^7.0.1",
    "react-error-html": "^0.2.2",
    "react-hot-loader": "^4.6.3",
    "regenerator-runtime": "^0.13.1",
    "source-map-support": "^0.5.9",
    "webpack-dev-server": "3.1.14"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-react": "7.0.0",
    "@types/webpack-dev-server": "3.1.1",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "3.0.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-pob": "24.0.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-typescript": "0.14.0",
    "pob-babel": "23.5.5",
    "prettier": "1.16.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "21.0.2",
    "webpack": "4.29.3"
  }
}
