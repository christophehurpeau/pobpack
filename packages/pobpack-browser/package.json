{
  "name": "pobpack-browser",
  "version": "6.1.0",
  "description": "Build and run browser projects with webpack and webpack-dev-server.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/pobpack.git",
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node8.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node8-dev.es.js"
  },
  "bin": {
    "pobpack-browser": "./pobpack-browser"
  },
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "pobpack-types": "^2.1.0",
    "pobpack-utils": "^6.1.0",
    "react-dev-utils": "^5.0.2",
    "react-error-html": "^0.2.2",
    "react-hot-loader": "^4.3.12",
    "regenerator-runtime": "^0.13.1",
    "source-map-support": "^0.5.9",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "webpack": "^4.26.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack-dev-server": "^3.1.1",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "2.0.2",
    "eslint": "5.9.0",
    "eslint-config-pob": "24.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.13.0",
    "pob-babel": "^23.4.0",
    "prettier": "1.15.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1",
    "webpack": "4.26.0"
  }
}
