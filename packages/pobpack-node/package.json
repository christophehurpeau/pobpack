{
  "name": "pobpack-node",
  "version": "9.0.0",
  "description": "Build and run node projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-node"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-node": "./pobpack-node"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.ts --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "cli"
    ]
  },
  "peerDependencies": {
    "webpack": "^4.44.2"
  },
  "dependencies": {
    "babel-preset-latest-node": "^4.0.1",
    "debounce": "^1.2.0",
    "pobpack-types": "^4.1.2",
    "pobpack-utils": "^9.0.0",
    "source-map-support": "^0.5.16",
    "springbokjs-daemon": "^5.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@pob/eslint-config": "37.2.0",
    "@pob/eslint-config-babel-node": "37.2.0",
    "@pob/eslint-config-node": "37.2.0",
    "@pob/eslint-config-typescript": "37.2.0",
    "@types/debounce": "1.2.0",
    "@types/webpack-node-externals": "1.7.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "eslint": "6.8.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-unicorn": "18.0.1",
    "pob-babel": "24.12.0",
    "prettier": "2.1.2"
  }
}
