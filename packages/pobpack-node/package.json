{
  "name": "pobpack-node",
  "version": "5.0.6",
  "description": "Build and run node projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/pobpack.git",
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node8.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node8-dev.es.js"
  },
  "bin": {
    "pobpack-node": "./pobpack-node"
  },
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "tsc --noEmit && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "babel-preset-latest-node": "^2.0.0-beta.3",
    "pobpack-types": "^1.0.1",
    "pobpack-utils": "^5.3.1",
    "source-map-support": "^0.5.4",
    "springbokjs-daemon": "^2.7.0",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "webpack": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@types/webpack-node-externals": "^1.6.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^19.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "^0.11.0",
    "pob-babel": "^22.2.4",
    "prettier": "^1.12.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  }
}
