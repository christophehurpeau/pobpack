{
  "name": "pobpack-node",
  "version": "7.4.1",
  "description": "Build and run node projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-node"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-node": "./pobpack-node"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "peerDependencies": {
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "babel-preset-latest-node": "^3.0.1",
    "pobpack-types": "^3.3.0",
    "pobpack-utils": "^7.4.0",
    "source-map-support": "^0.5.10",
    "springbokjs-daemon": "^3.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/webpack-node-externals": "1.6.3",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@typescript-eslint/parser": "1.3.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-pob": "28.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "pob-babel": "23.6.3",
    "prettier": "1.16.4",
    "typescript": "3.3.3"
  }
}
