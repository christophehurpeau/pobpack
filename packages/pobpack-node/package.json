{
  "name": "pobpack-node",
  "version": "7.2.3",
  "description": "Build and run node projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/pobpack.git",
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./cli.js": "./dist/cli-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./cli.js": "./dist/cli-node10-dev.es.js"
  },
  "bin": {
    "pobpack-node": "./pobpack-node"
  },
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "peerDependencies": {
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "babel-preset-latest-node": "^3.0.1",
    "pobpack-types": "^3.2.1",
    "pobpack-utils": "^7.3.1",
    "source-map-support": "^0.5.9",
    "springbokjs-daemon": "^3.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/webpack-node-externals": "1.6.3",
    "babel-eslint": "10.0.1",
    "eslint": "5.11.0",
    "eslint-config-pob": "24.0.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-typescript": "0.14.0",
    "pob-babel": "23.5.5",
    "prettier": "1.15.3",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "21.0.2"
  }
}
