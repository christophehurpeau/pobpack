{
  "name": "pobpack-utils",
  "version": "8.2.0",
  "description": "Utils for building projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-utils"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.ts --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.1.6",
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "@types/webpack": "^4.41.0",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "colorette": "^1.1.0",
    "find-up": "^4.0.0",
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "nightingale-logger": "^10.0.1",
    "pobpack-types": "^4.1.1",
    "progress": "^2.0.3",
    "react-dev-utils": "^10.0.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@pob/eslint-config": "37.1.0",
    "@pob/eslint-config-babel-node": "37.0.2",
    "@pob/eslint-config-node": "37.0.2",
    "@pob/eslint-config-typescript": "37.1.0",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.4",
    "@types/find-up": "4.0.0",
    "@types/progress": "2.0.3",
    "@types/react-dev-utils": "9.0.4",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "babel-preset-latest-node": "4.1.0",
    "eslint": "6.8.0",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-unicorn": "18.0.1",
    "pob-babel": "24.12.0",
    "prettier": "2.0.5"
  }
}
