{
  "name": "pobpack-utils",
  "version": "7.8.1",
  "description": "Utils for building projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-utils"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"]
  },
  "peerDependencies": {
    "@babel/core": "^7.1.6",
    "webpack": "^4.28.1"
  },
  "dependencies": {
    "@types/webpack": "^4.4.27",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "colorette": "^1.0.7",
    "find-up": "^4.0.0",
    "nightingale": "^9.2.3",
    "nightingale-console": "^6.0.1",
    "nightingale-logger": "^6.2.2",
    "pobpack-types": "^3.5.2",
    "progress": "^2.0.3",
    "react-dev-utils": "^9.0.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@pob/eslint-config-babel-node": "32.1.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.3",
    "@types/find-up": "2.1.1",
    "@types/progress": "2.0.3",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.3.0",
    "pob-babel": "23.22.1",
    "prettier": "1.18.2"
  }
}
