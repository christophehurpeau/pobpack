{
  "name": "pobpack-utils",
  "version": "9.0.0",
  "description": "Utils for building projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-utils"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint:eslint": "eslint --ext .js,.ts --report-unused-disable-directives --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.1.6",
    "webpack": "^4.28.1 || ^5.0.0"
  },
  "dependencies": {
    "@types/webpack": "^4.41.0",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "colorette": "^1.1.0",
    "find-up": "^4.0.0",
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "nightingale-logger": "^10.0.1",
    "pobpack-types": "^4.1.2",
    "progress": "^2.0.3",
    "react-dev-utils": "~11.0.0-next.77",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@pob/eslint-config": "41.0.4",
    "@pob/eslint-config-node": "41.0.4",
    "@pob/eslint-config-typescript": "41.0.4",
    "@pob/eslint-config-typescript-node": "41.0.4",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.4",
    "@types/find-up": "4.0.0",
    "@types/progress": "2.0.3",
    "@types/react-dev-utils": "9.0.4",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-preset-latest-node": "5.1.1",
    "eslint": "7.13.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "23.0.0",
    "pob-babel": "26.0.1",
    "prettier": "2.1.2",
    "rollup": "2.33.2"
  }
}
