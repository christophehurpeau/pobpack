    "nightingale": "^8.0.8",
    "nightingale-console": "^4.0.6",
    "nightingale-logger": "^5.1.0",
    "pobpack-types": "^1.0.1",
    "progress": "^2.0.0",
    "react-dev-utils": "^5.0.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "webpack": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.2",
