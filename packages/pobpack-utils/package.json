    "test": "echo \"No tests\"",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
