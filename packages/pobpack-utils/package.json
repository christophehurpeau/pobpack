{
  "name": "pobpack-utils",
  "version": "12.0.0",
  "description": "Utils for building projects with webpack.",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-utils"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.mjs,.ts --report-unused-disable-directives --quiet packages/pobpack-utils",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.1.6",
    "webpack": "^4.44.2"
  },
  "dependencies": {
    "@types/node": "14.14.43",
    "@types/webpack": "^4.41.26",
    "babel-loader": "^8.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "colorette": "^1.1.0",
    "find-up": "^5.0.0",
    "nightingale": "^11.0.5",
    "nightingale-console": "^11.0.5",
    "nightingale-logger": "^11.0.5",
    "pobpack-types": "^12.0.0",
    "progress": "^2.0.3",
    "react-dev-utils": "11.0.1",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.5",
    "@types/find-up": "4.0.0",
    "@types/progress": "2.0.3",
    "@types/react-dev-utils": "9.0.5",
    "babel-preset-latest-node": "5.5.1",
    "pob-babel": "26.4.0",
    "rollup": "2.47.0"
  }
}
