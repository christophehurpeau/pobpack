{
  "name": "pobpack-browser-example",
  "private": true,
  "version": "6.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pobpack.git",
    "directory": "packages/pobpack-browser-example"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "engines": {
    "node": ">=12.10.0"
  },
  "scripts": {
    "build": "pobpack-browser build && gzip -fk9 public/*.js && lzma -fke9 --threads 0 public/*.js && brotli -Z public/*.js && echo",
    "lint:eslint": "eslint --ext .js,.ts,.tsx --report-unused-disable-directives --quiet src",
    "serve": "cp -f index.html public/ && serve -s public",
    "start": "pobpack-browser"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "pobpack-browser": "^9.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "workbox-core": "^5.1.4",
    "workbox-precaching": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@pob/eslint-config": "41.0.4",
    "@pob/eslint-config-node": "41.0.4",
    "@pob/eslint-config-typescript": "41.0.4",
    "@pob/eslint-config-typescript-react": "41.0.4",
    "@types/object-assign": "4.0.30",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "eslint": "7.13.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "23.0.0",
    "prettier": "2.1.2",
    "serve": "11.3.2",
    "webpack": "5.5.0"
  }
}
