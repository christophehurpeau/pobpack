    "directory": "packages/pobpack-browser-example"
  },
  "homepage": "https://github.com/christophehurpeau/pobpack",
  "scripts": {
    "build": "pobpack-browser build && gzip -fk9 public/*.js && lzma -fke9 --threads 0 public/*.js && brotli -Z public/*.js",
    "lint": "tsc && eslint --ext .ts,.tsx src/",
    "start": "pobpack-browser"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "pobpack-browser": "^7.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@pob/eslint-config-babel": "29.1.0",
    "@pob/eslint-config-typescript": "29.1.0",
    "@pob/eslint-config-typescript-react": "29.1.0",
    "eslint": "5.16.0",
    "prettier": "1.16.4",
    "typescript": "3.4.1",
    "webpack": "4.29.6"
  }
}
